<Window x:Class="SkeletonJoints.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkeletonJoints"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
        Loaded="Window_Loaded" Unloaded="Window_Unloaded">
    
    <Grid>
        <Image Name="img" Width="640" Height="480" />
        <Canvas Name="canvas" Width="640" Height="480" />
        <TextBlock Name="tblHeight" Text="Height: N/A" FontSize="25" Margin="5" />
        <TextBlock Name="tblArmExtendWidth" Text="Height: N/A" FontSize="25" Margin="5,30,5,5" />
    </Grid>
</Window>
